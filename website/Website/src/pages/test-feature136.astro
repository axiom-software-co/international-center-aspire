---
import Layout from '../layouts/Layout.astro';
import Feature136Simple from '../components/blocks/Feature136Simple.vue';
import { loadHeroServicesData } from '../lib/navigation-data';

// Load hero services data during SSR
const heroServicesData = await loadHeroServicesData();
console.log('Test page - loaded hero data:', heroServicesData);
---

