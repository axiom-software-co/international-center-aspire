{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Database=international_center;Username=postgres;Password=postgres"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Service.Migration.Orchestrator": "Debug"
    },
    "Console": {
      "LogToStandardErrorThreshold": "Error"
    }
  },
  "MigrationOrchestrator": {
    "StopOnFirstFailure": true,
    "TimeoutMinutes": 30,
    "EnableProductionSafetyChecks": true,
    "BackupBeforeMigration": false,
    "DryRunMode": false,
    "Domains": [
      {
        "Name": "Services",
        "Enabled": true,
        "Priority": 100,
        "TimeoutMinutes": 15,
        "SkipDependencyValidation": false,
        "ContextTypeName": "ServicesDbContext",
        "Properties": {
          "Description": "Services domain migrations for Public and Admin APIs",
          "Owner": "Services Team",
          "CriticalityLevel": "High"
        }
      }
    ],
    "Database": {
      "ConnectionString": "Host=localhost;Database=international_center;Username=postgres;Password=postgres",
      "CommandTimeoutSeconds": 300,
      "RetryAttempts": 3,
      "RetryDelaySeconds": 5,
      "EnableConnectionPooling": false,
      "MigrationHistoryTableName": "__EFMigrationsHistory"
    }
  }
}