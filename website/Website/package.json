{
  "name": "website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --host 0.0.0.0 --port 4321",
    "dev:windows": "astro dev --host 0.0.0.0 --port 4321",
    "dev:astro-only": "astro dev --host 0.0.0.0 --port 4321",
    "dev:check": "node --version && echo 'Node version check complete'",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "astro check",
    "lint:check": "astro check && eslint . --ext .ts,.tsx,.astro",
    "format:check": "prettier --check . --ignore-path .gitignore",
    "format": "prettier --write . --ignore-path .gitignore",
    "typecheck": "tsc --noEmit",
    "test": "vitest --workspace",
    "test:unit": "vitest run --workspace --project unit",
    "test:ui": "vitest --ui --workspace",
    "test:coverage": "vitest run --coverage --workspace --project unit",
    "test:watch": "vitest --watch --workspace --project unit",
    "test:integration": "vitest run --workspace --project integration",
    "test:integration:ui": "vitest --ui --workspace --project integration",
    "test:integration:watch": "vitest --watch --workspace --project integration",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:smoke": "playwright test tests/smoke --reporter=line",
    "test:newsletter": "vitest run src/test/integration/newsletter-cta.integration.test.ts",
    "test:newsletter:watch": "vitest watch src/test/integration/newsletter-cta.integration.test.ts",
    "test:newsletter:e2e": "playwright test tests/newsletter-signup.spec.ts",
    "test:newsletter:e2e:headed": "playwright test tests/newsletter-signup.spec.ts --headed",
    "test:newsletter:all": "bun run test:newsletter && bun run test:newsletter:e2e",
    "test:all": "vitest run --workspace"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/node": "^9.4.2",
    "@astrojs/tailwind": "^6.0.2",
    "@astrojs/vue": "^5.1.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/leaflet": "^1.9.20",
    "@vueuse/core": "^13.7.0",
    "astro": "^5.12.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "leaflet": "^1.9.4",
    "lucide-vue-next": "^0.540.0",
    "node-fetch": "^3.3.2",
    "radix-vue": "^1.9.17",
    "shadcn-vue": "^2.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "1.0.7",
    "typescript": "^5.9.2",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.1",
    "@playwright/test": "^1.51.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/browser": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.19.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "firebase-tools": "^14.12.1",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "typescript-eslint": "^8.40.0",
    "vitest": "^3.2.4"
  }
}
