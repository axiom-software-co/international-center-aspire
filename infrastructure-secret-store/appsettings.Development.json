{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Infrastructure.SecretStore": "Debug",
      "Azure": "Information"
    }
  },
  "SecretStore": {
    "Enabled": true,
    "Provider": "AzureKeyVault",
    "VaultUri": "https://your-dev-keyvault-name.vault.azure.net/",
    "VaultName": "your-dev-keyvault-name",
    "DefaultTimeout": "00:00:15",
    "MaxRetryAttempts": 2,
    "RetryDelay": "00:00:01",
    "EnableCaching": false,
    "CacheDuration": "00:05:00",
    "EnableMetrics": true,
    "EnableHealthChecks": true,
    "Authentication": {
      "Provider": "DefaultAzureCredential",
      "ClientId": null,
      "ClientSecret": null,
      "TenantId": null,
      "CertificateThumbprint": null,
      "CertificatePath": null,
      "UseDefaultAzureCredential": true,
      "Authority": "https://login.microsoftonline.com/",
      "AdditionallyAllowedTenants": null,
      "DisableInstanceDiscovery": false,
      "EnableCae": false
    },
    "Rotation": {
      "EnableAutoRotation": false,
      "DefaultRotationInterval": "7.00:00:00",
      "RotationCheckInterval": "01:00:00",
      "MaxConcurrentRotations": 2,
      "RotationTimeout": "00:05:00",
      "NotifyOnRotation": false,
      "NotificationEndpoints": null,
      "KeepOldVersions": true,
      "MaxVersionsToKeep": 3
    },
    "Performance": {
      "MaxConcurrentRequests": 10,
      "RequestTimeout": "00:00:15",
      "EnableConnectionPooling": true,
      "MaxConnectionsPerEndpoint": 5,
      "PooledConnectionIdleTimeout": "00:01:00",
      "PooledConnectionLifetime": "00:05:00",
      "EnableResponseCompression": true
    }
  }
}